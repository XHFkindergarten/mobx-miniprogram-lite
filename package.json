{
  "name": "mobx-miniprogram-lite",
  "version": "0.0.3-alpha.2",
  "description": "lightweight mobx-binding for wechat-miniprogram",
  "main": "lib/index.common.js",
  "module": "lib/index.js",
  "types": "lib/index.d.ts",
  "scripts": {
    "tsc": "tsc && tsc-alias",
    "build": "tsc && cross-env NODE_ENV=production node ./scripts/build.js",
    "dev": "tsc -w & tsc-alias -w & cross-env NODE_ENV=development node ./scripts/build.js -w",
    "cz": "git-cz",
    "release": "standard-version -t mobx-miniprogram-lite --release-as patch",
    "release:alpha": "standard-version -t mobx-miniprogram-lite --release-as patch --prerelease alpha",
    "test": "jest",
    "prepare": "husky install"
  },
  "keywords": [
    "mobx",
    "miniprogram",
    "wechat"
  ],
  "author": "XHFkindergarten@gmail.com",
  "license": "ISC",
  "dependencies": {
    "mobx": "^6.10.0"
  },
  "devDependencies": {
    "@jest/types": "^29.6.3",
    "@swc/core": "^1.3.80",
    "@swc/jest": "^0.2.29",
    "@types/jest": "^29.5.4",
    "@types/node": "^20.5.7",
    "@types/wechat-miniprogram": "^3.4.1",
    "cross-env": "^7.0.3",
    "esbuild": "^0.19.2",
    "husky": "^8.0.3",
    "jest": "^29.6.4",
    "jest-environment-jsdom": "^29.6.4",
    "miniprogram-simulate": "^1.6.0",
    "prettier": "^3.0.2",
    "standard-version": "^9.5.0",
    "ts-node": "^10.9.1",
    "tsc-alias": "^1.8.7",
    "typescript": "^5.1.6"
  }
}
